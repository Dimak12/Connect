<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>Connect.Signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileDescriptiion</servlet-name>
        <servlet-class>Connect.ProfileDescriptiion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Connect.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoggedUser</servlet-name>
        <servlet-class>Connect.LoggedUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Posts</servlet-name>
        <servlet-class>Connect.Posts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Connect.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>Connect.LogOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Follow</servlet-name>
        <servlet-class>Connect.Follow</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Unfollow</servlet-name>
        <servlet-class>Connect.Unfollow</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateProfile</servlet-name>
        <servlet-class>Connect.UpdateProfile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/Signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileDescriptiion</servlet-name>
        <url-pattern>/ProfileDescriptiion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoggedUser</servlet-name>
        <url-pattern>/LoggedUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Posts</servlet-name>
        <url-pattern>/Posts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/LogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Follow</servlet-name>
        <url-pattern>/Follow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Unfollow</servlet-name>
        <url-pattern>/Unfollow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateProfile</servlet-name>
        <url-pattern>/UpdateProfile</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
